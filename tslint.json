{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "sprk",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "sprk",
      "kebab-case"
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "ban-keywords",
      "allow-pascal-case"
    ]
  }
}
